#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/@capacitor+cli@6.2.1/node_modules/@capacitor/cli/bin/node_modules:/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/@capacitor+cli@6.2.1/node_modules/@capacitor/cli/node_modules:/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/@capacitor+cli@6.2.1/node_modules/@capacitor/node_modules:/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/@capacitor+cli@6.2.1/node_modules:/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/@capacitor+cli@6.2.1/node_modules/@capacitor/cli/bin/node_modules:/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/@capacitor+cli@6.2.1/node_modules/@capacitor/cli/node_modules:/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/@capacitor+cli@6.2.1/node_modules/@capacitor/node_modules:/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/@capacitor+cli@6.2.1/node_modules:/Users/mdearaujo/Documents/Projetos/git/QRCraft-Studio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@capacitor/cli/bin/capacitor" "$@"
else
  exec node  "$basedir/../@capacitor/cli/bin/capacitor" "$@"
fi
